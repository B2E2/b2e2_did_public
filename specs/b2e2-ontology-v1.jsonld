    "FISSION": {
      "@id": "FISSION",
      "@comment": "Nuclear fission – energy from splitting atomic nuclei."
    },
    "FUSION": {
      "@id": "FUSION",
      "@comment": "Nuclear fusion – energy from fusing atomic nuclei."
    },
    "PV": {

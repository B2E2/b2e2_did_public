      "@id": "ONSHOREWIND",
      "@comment": "Onshore wind – wind turbines located on land generating electricity."
    },
    "OFFSHOREWIND": {
      "@id": "OFFSHOREWIND",
      "@comment": "Offshore wind – wind turbines located at sea."
    },
    "WAVE": {
      "@id": "WAVE",
      "@comment": "Wave – energy derived from ocean surface waves."
    },
    "NATURALGAS": {
      "@id": "NATURALGAS",
      "@comment": "Natural gas – fossil fuel used in thermal power generation."
    },
    "BIOGAS": {
      "@id": "BIOGAS",
      "@comment": "Biogas – produced from organic matter through anaerobic digestion."
    },

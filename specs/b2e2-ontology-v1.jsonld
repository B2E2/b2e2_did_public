    },
    "BLACKCOAL": {
      "@id": "BLACKCOAL",
      "@comment": "Black coal – harder and higher energy coal compared to brown coal."
    },
    "OIL": {
      "@id": "OIL",
      "@comment": "Oil – petroleum used in energy production."
    },
    "DIESEL": {
      "@id": "DIESEL",

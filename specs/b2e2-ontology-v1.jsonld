    },
    "GARBAGE": {
      "@id": "GARBAGE",
      "@comment": "Garbage – waste used for incineration and energy recovery."
    },
    "HYDROR": {
      "@id": "HYDROR",
      "@comment": "Run-of-river hydro – uses river flow without large reservoirs to generate electricity."
    },
    "HYDROPS": {
      "@id": "HYDROPS",
      "@comment": "Pumped-storage hydro – stores energy by pumping water to a higher elevation and releasing it to generate electricity when needed."
    },
    "HYDROWS": {
      "@id": "HYDROWS",
      "@comment": "Reservoir hydro – conventional hydroelectric power from large dammed reservoirs."
    },

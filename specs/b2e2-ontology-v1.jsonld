    },
    "PVP": {
      "@id": "PVP",
      "@comment": "Photovoltaics park – large-scale solar farms generating electricity from sunlight."
    },
    "CSP": {
      "@id": "CSP",
      "@comment": "Concentrated Solar Power – mirrors focus sunlight to produce thermal energy and generate electricity."
    },
    "GEOTHERMAL": {
      "@id": "GEOTHERMAL",
      "@comment": "Geothermal – energy from Earth's internal heat."
    },
